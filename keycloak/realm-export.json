{
  "id": "blazor-dev",
  "realm": "blazor-dev",
  "enabled": true,
  "roles": {
    "realm": [
      {
        "name": "app-admin",
        "description": "Admin role for the Blazor app"
      },
      {
        "name": "app-user",
        "description": "User role for the Blazor app"
      }
    ]
  },
  "clients": [
    {
      "clientId": "blazor-app",
      "name": "Blazor App",
      "description": "Client for the Blazor application",
      "secret": "blazor-app-secret",
      "protocol": "openid-connect",
      "publicClient": false,
      "clientAuthenticatorType": "client-secret",
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": false,
      "redirectUris": [
        "https://localhost:7235/signin-oidc"
      ],
      "webOrigins": [
        "https://localhost:7235"
      ],
      "attributes": {
        "post.logout.redirect.uris": "https://localhost:7235/*"
      },
      "protocolMappers": [
        {
          "name": "roles",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-realm-role-mapper",
          "consentRequired": false,
          "config": {
            "claim.name": "roles",
            "jsonType.label": "String",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "userinfo.token.claim": "true",
            "multivalued": "true"
          }
        }
      ]
    }
  ],
  "users": [
    {
      "username": "alice",
      "enabled": true,
      "firstName": "Alice",
      "lastName": "Smith",
      "email": "alicesmith@test.com",
      "emailVerified": true,
      "credentials": [
        {
          "type": "password",
          "value": "Pass123$",
          "temporary": false
        }
      ],
      "realmRoles": [
        "app-user",
        "offline_access"
      ]
    },
    {
      "username": "bob",
      "enabled": true,
      "firstName": "Bob",
      "lastName": "Admin",
      "email": "bobadmin@test.com",
      "emailVerified": true,
      "credentials": [
        {
          "type": "password",
          "value": "Pass123$",
          "temporary": false
        }
      ],
      "realmRoles": [
        "app-admin",
        "app-user",
        "offline_access"
      ]
    }
  ]
}